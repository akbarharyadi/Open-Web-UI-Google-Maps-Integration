# Google Maps API Keys
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Backend Configuration
BACKEND_API_URL=http://fastapi-backend:8000/api/maps
BROWSER_API_URL=http://localhost:8000/api/maps
CORS_ORIGINS=http://localhost:3000
APP_NAME=Open Web UI Maps API
LOG_LEVEL=INFO

# Open WebUI Configuration
OLLAMA_BASE_URL=http://ollama:11434
WEBUI_NAME=Open Web UI Maps AI
ENABLE_SIGNUP=true
DEFAULT_MODELS=llama3.2:latest
ENABLE_RAG=false
ENABLE_WEB_SEARCH=false
DEFAULT_LOCALE=en-US
SHOW_MESSAGE_DURATION=0

# Iframe Security Settings
ENABLE_IFRAME_SANDBOX=false
